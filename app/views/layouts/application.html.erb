<!DOCTYPE html>
<html>
  <head>
    <title>FitMatrix</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application'%> 
    <%= javascript_include_tag 'auth'%>  
    <%= javascript_include_tag 'search2'%>  
    <%= favicon_link_tag "favicon-jumprope.png" %>
    <link href="https://fonts.googleapis.com/css?family=Audiowide|Exo+2|Josefin+Sans|Jura|Krona+One|Monoton|Orbitron|Raleway+Dots|Righteous" rel="stylesheet"> 

  </head>

  <body> 
  
        
        <%= yield %>
     
        <div class="container">
        <% flash.each do |key, value| %>
          <div class="alert alert-<%= key %>">
            <%= value %>
          </div>
        <% end %>
      </div>

  
   
    </div>
  
    
  
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
    <script src="https://apis.google.com/js/client.js?onload=googleApiClientReady"></script>
    <script src="https://apis.google.com/js/client.js?onload=init"></script>
    <script src="https://code.jquery.com/jquery-2.1.3.min.js"></script>
    <script src="https://apis.google.com/js/client.js?onload=googleApiClientReady"></script>

  </body>
</html>
